{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "html/**",
          "css/**",
          "js/**",
          "images/**",
          "data/**",
          "data/local-data/**"
        ]
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.js" }
  ]
}
